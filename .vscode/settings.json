{
    "files.exclude": {
        "reports/": true,
        "out/": true
    },
    "search.exclude": {
        "out/": true
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "[yaml]": {
        "editor.tabSize": 2
    },
    "[markdown]": {
        "editor.tabSize": 2
    },
    "files.associations": {
        "**/.azure-pipelines/*.yaml": "azure-pipelines"
    },
    "cSpell.words": [
        "OWASP",
        "VNET",
        "cmdlet",
        "vnets"
    ]
}
